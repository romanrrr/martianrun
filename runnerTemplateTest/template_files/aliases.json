[
  {
    "name": "symbols",
    "rules": [
      {
        "like": "\\D+$"
      }
    ],
    "error": "Symbols only."
  },
  {
    "name": "email",
    "rules": [
      {
        "like": "^(([^<>()\\[\\]\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))"
      }
    ],
    "error": "Invalid Email."
  },
  {
    "name": "numbersOnly",
    "rules": [
      {
        "like": "^[0-9,.]{1,}$"
      }
    ],
    "error": "Numbers only."
  }
]